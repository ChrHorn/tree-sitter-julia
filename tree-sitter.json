  "grammars": [
    {
      "name": "julia",
      "camelcase": "Julia",
      "path": ".",
      "scope": "source.julia",
      "injection-regex": "^(jl|julia)$",
      "file-types": [
        "jl"
      ],
      "highlights": "queries/query/highlights.scm",
      "injections": "queries/query/injections.scm",
      "locals": "queries/query/locals.scm"
    }
  ],
